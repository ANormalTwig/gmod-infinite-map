	"$model" "1"

	"$scale" "[1 1]"

	"$bumpmap" "dev/water_dudv"
	"$normalmap" "infmap/dx80_tfwater001_normal"
	"$bumpframe" "0"


	"Proxies"
	{
		"AnimatedTexture"
